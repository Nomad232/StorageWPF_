<Page x:Class="StorageWPF.Views.GeneralInfoPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:StorageWPF.Views"
      xmlns:oxy="http://oxyplot.org/wpf"
      mc:Ignorable="d" 
      d:DesignHeight="485" d:DesignWidth="765"
      Title="GeneralInfoPage">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <oxy:PlotView Background="{StaticResource BackgroundWindowBrush}" Model="{Binding PlotModelCount}" Grid.Row="1" />
        <oxy:PlotView Grid.Column="1" Background="{StaticResource BackgroundWindowBrush}" Model="{Binding PlotModelSum}" Grid.Row="1" />
    </Grid>
</Page>